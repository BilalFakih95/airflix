{
    "name": "airflix/airflix",
    "description": "An AirPlay friendly web interface to stream your movies and TV shows from a home server.",
    "keywords": ["airflix", "airplay", "tv", "movies"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Brian Wells",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "~5.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.3",
        "laravel/tinker": "^1.0",
        "league/fractal": "^0.14.0",
        "league/glide-laravel": "^1.0",
        "php-tmdb/laravel": "~1.0",
        "predis/predis": "~1.0",
        "ramsey/uuid": "^3.3",
        "spatie/laravel-cors": "^1.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^2.2",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-test-factory-helper": "^0.5.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Airflix\\": "Airflix/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
